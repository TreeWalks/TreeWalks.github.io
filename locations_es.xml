      <item>{{ post.title }}|{{ post.description }}|{{ post.content }}|{{ post.url | absolute_url }}|{{ post.alternate_names }}|{{ post.fun_fact }}|{{ post.native }}</item>
    {% endfor %}
  </string-array>
</resources>
